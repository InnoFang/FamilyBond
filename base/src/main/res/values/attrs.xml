<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- GroupButtonView -->
    <declare-styleable name="GroupButtonView">
        <attr name="groupBtnNameText" format="string"/>
        <attr name="groupBtnCodeText" format="string"/>
    </declare-styleable>

    <declare-styleable name="FabTagLayout">
        <attr name="tagText" format="string" />
    </declare-styleable>

    <attr name="animationMode">
        <enum name="fade" value="0"/>
        <enum name="Scale" value="1"/>
        <enum name="bounce" value="2"/>
    </attr>
    <attr name="position" >
        <enum name="left_top" value="0"/>
        <enum name="left_bottom" value="1"/>
        <enum name="right_top" value="2"/>
        <enum name="right_bottom" value="3"/>
    </attr>
    <declare-styleable name="FloatingActionButtonPlus">
        <attr name="animationMode"/>
        <attr name="position"/>
        <attr name="mBackgroundColor" format="color"/>
        <attr name="switchFabIcon" format="reference"/>
        <attr name="switchFabColor" format="color"/>
        <attr name="animationDuration" format="integer"/>
    </declare-styleable>

</resources>